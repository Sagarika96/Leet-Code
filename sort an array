class Solution {
    public int[] sortArray(int[] nums) {
        
        
        int [] copy= new int[nums.length];
        
        for(int i=0;i<nums.length;i++)
        {
            copy[i]=nums[i];
        }
        
        Arrays.sort(copy);
        
        for(int i=0;i<nums.length;i++)
        {
            nums[i]=copy[i];
        }
        
        return nums;
        
    
        
        
    }
}
